{
  "name": "storage-web-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 3011 --host 0.0.0.0",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop storage-web-app",
    "pm2:restart": "pm2 restart storage-web-app",
    "pm2:delete": "pm2 delete storage-web-app",
    "pm2:logs": "pm2 logs storage-web-app",
    "pm2:status": "pm2 status",
    "deploy": "npm run build && pm2 restart storage-web-app",
    "docker:build": "docker build -t storage-web-app .",
    "docker:run": "docker run -d -p 3011:3011 --name storage-web storage-web-app",
    "docker:deploy": "docker-compose up -d --build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.18.2",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
